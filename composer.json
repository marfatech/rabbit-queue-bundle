{
    "name":        "marfatech/rabbit-queue-bundle",
    "description": "Provides possibility for working with rabbitMQ queue by consumers system",
    "type":        "symfony-bundle",
    "keywords":    ["symfony-bundle", "queue", "rabbitmq", "consumer"],
    "license":     "proprietary",

    "require": {
        "php":                              "^8.0",
        "ext-json":                         "*",
        "ext-pcntl":                        "*",
        "symfony/config":                   "~4.4||~5.4||~6.0",
        "symfony/console":                  "~4.4||~5.4||~6.0",
        "symfony/dependency-injection":     "~4.4||~5.4||~6.0",
        "symfony/http-kernel":              "~4.4||~5.4||~6.0",
        "php-amqplib/php-amqplib":          "^2.12"
    },

    "require-dev": {
        "psr/log":                          "~1.0",
        "phpunit/phpunit":                  "^^7.0||^8.0||^9.0",
        "symfony/monolog-bundle":           "^3.7",
        "symfony/framework-bundle":         "^5.3||~6.0"
    },

    "suggest": {
        "symfony/monolog-bundle":   "Provides advanced logging mechanism",
        "symfony/symfony":          "Allows more advanced functionality with Symfony"
    },

    "autoload": {
        "psr-4": {
            "MarfaTech\\Bundle\\RabbitQueueBundle\\": ""
        }
    }
}
